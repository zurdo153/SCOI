	<queryString>
		<![CDATA[declare @folio_orden_de_pago int
set @folio_orden_de_pago =2;
select 
right('000000'+convert(varchar(20),tb_orden_de_pago_en_efectivo.folio),6) as folio
,tb_orden_de_pago_en_efectivo.cantidad
,dbo.CantidadConLetra(tb_orden_de_pago_en_efectivo.cantidad) as cantidad_con_letra
,convert(varchar(20),tb_orden_de_pago_en_efectivo.fecha,103) as fecha
,tb_orden_de_pago_en_efectivo.concepto
,tb_autorizan_orden_de_pago.nombre_completo as autorizo
,case when tb_orden_de_pago_en_efectivo.tipo_beneficiario = 'P' then 'PROVEEDOR' else 'EMPLEADO' end as tipo_beneficiario
,case when tb_orden_de_pago_en_efectivo.tipo_beneficiario = 'P' then (select nombre+' '+ap_paterno+' '+ap_materno as beneficiario FROM tb_proveedores where folio_proveedor = tb_orden_de_pago_en_efectivo.folio_beneficiario) else (SELECT nombre+' '+ap_paterno+' '+ap_materno FROM tb_empleado where folio = tb_orden_de_pago_en_efectivo.folio_beneficiario) end as beneficiario
,tb_empleado.nombre+' '+tb_empleado.ap_paterno+' '+tb_empleado.ap_materno as elaboro
,isnull(tb_establecimiento.nombre,'SIN ESTABLECIMIENTO') as establecimiento
from tb_orden_de_pago_en_efectivo
inner join tb_autorizan_orden_de_pago on tb_autorizan_orden_de_pago.folio = tb_orden_de_pago_en_efectivo.folio_autorizo
inner join tb_empleado on tb_empleado.folio = tb_orden_de_pago_en_efectivo.usuario
left outer join tb_establecimiento on tb_establecimiento.folio = tb_orden_de_pago_en_efectivo.folio_establecimiento
where tb_orden_de_pago_en_efectivo.folio = @folio_orden_de_pago]]>
	</queryString>
