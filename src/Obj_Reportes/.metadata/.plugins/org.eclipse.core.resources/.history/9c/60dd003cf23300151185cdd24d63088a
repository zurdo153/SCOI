	<queryString>
		<![CDATA[declare @fechaIn datetime, @fechaFin datetime

set @fechaIn  = '01/06/2015 00:00:00';
set @fechaFin = '26/07/2015 23:59:00';

select 
		al_cort.folio_corte
		,t_emp.nombre+' '+t_emp.ap_paterno+' '+t_emp.ap_materno as empleado
		,t_estab.nombre as establecimiento
		,convert(varchar(20),al_cort.fecha_de_corte,103)+' '+convert(varchar(20),al_cort.fecha_de_corte,108) as fecha_corte
		,case  when (al_cort.status_de_cobro_seguridad = 2 and al_cort.diferencia_seguridad_sugerido <> 0) then al_cort.diferencia_seguridad_sugerido
															when (al_cort.status_de_cobro_seguridad = 2 and al_cort.diferencia_seguridad_sugerido = 0 and al_cort.diferencia_auditoria <> 0) then al_cort.diferencia_auditoria
															when (al_cort.status_de_cobro_auditoria = 2 and al_cort.diferencia_auditoria <> 0) then al_cort.diferencia_auditoria
															else al_cort.diferiencia_de_corte
														end as diferencia
		,case  when (al_cort.status_de_cobro_seguridad = 2 and al_cort.diferencia_seguridad_sugerido <> 0) then convert(varchar(20),al_cort.fecha_reviso_seguridad,103)+' '+convert(varchar(20),al_cort.fecha_reviso_seguridad,108)
															else convert(varchar(20),al_cort.fecha_revision_auditoria,103)+' '+convert(varchar(20),al_cort.fecha_revision_auditoria,108)													 
														end as fecha_paso_a_cobro
		,case  when (al_cort.status_de_cobro_seguridad = 2 and al_cort.diferencia_seguridad_sugerido <> 0) then convert(varchar(20),al_cort.comentario_seguridad,103)+' '+convert(varchar(20),al_cort.comentario_seguridad,108)
															else al_cort.comentario_auditoria													 
														end as observacion

		,convert(varchar(20),al_cort.fecha_autorizacion_de_revision_de_corte,103)+' '+convert(varchar(20),al_cort.fecha_autorizacion_de_revision_de_corte,108) as fecha_autorizacion_de_revision_de_corte
		,case when al_cort.status = 1 then 'Vigente' else 'Cobrado' end status  --(2 es cobrado), (1 es vigente y lista de raya es 0)
		,al_cort.folio_lista_de_raya_cobro as folio_lr
		,'0' as abono
from tb_alimentacion_de_cortes al_cort
inner join tb_empleado t_emp on t_emp.folio = al_cort.folio_empleado
inner join tb_establecimiento t_estab on t_estab.folio = al_cort.folio_establecimiento_de_corte
where al_cort.status_autorizacion_corte = 1 and al_cort.fecha_de_corte >= @fechaIn and al_cort.fecha_de_corte <= @fechaFin
  
UNION ALL( SELECT '' as folio_corte
				  ,t_emp.nombre+' '+t_emp.ap_paterno+' '+t_emp.ap_materno as empleado
				  ,t_estab.nombre as establecimiento
				  ,'' fecha_corte
				  ,0 as diferencia
				  ,'' fecha_paso_a_cobro
				  ,'' as observacion
				  ,abn.fecha_de_modificacion as fecha_autorizacion_de_revision_de_corte
				  ,'' status
				  ,abn.lista_de_raya_del_abono 
				  ,abn.abono as abono
			FROM tb_abonos as abn
			inner join Tb_empleado as t_emp on t_emp.folio=abn.folio_empleado
			inner join tb_establecimiento t_estab on t_estab.folio = t_emp.establecimiento_id
			WHERE CONVERT(datetime,fecha_de_modificacion)>=@fechaIn  and CONVERT(datetime,fecha_de_modificacion)<=@fechaFin and abn.abono>0)

  order by empleado asc, fecha_autorizacion_de_revision_de_corte asc]]>
	</queryString>
	<field name="folio_corte" class="java.lang.String"/>
	<field name="empleado" class="java.lang.String"/>
	<field name="establecimiento" class="java.lang.String"/>
