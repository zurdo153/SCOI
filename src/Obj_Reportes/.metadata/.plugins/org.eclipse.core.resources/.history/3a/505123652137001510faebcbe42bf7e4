	<queryString language="SQL">
		<![CDATA[declare @fecha_recepcion varchar(20),@fecha_transferencia varchar(20)

,@recepcion varchar(50) set @recepcion='S214751'

set @fecha_recepcion=(select convert(varchar(20),movimientos_internos.fecha,103)+' '+convert(varchar(20),movimientos_internos.fecha,108)
                             from movimientos_internos with(nolock) where movimientos_internos.folio=@recepcion and movimientos_internos.transaccion='65')

set @fecha_transferencia=(select convert(varchar(20),movimientos_internos.fecha,103)+' '+convert(varchar(20),movimientos_internos.fecha,108)
                               from movimientos_internos with(nolock) where movimientos_internos.folio=(select folio_alterno from movimientos_internos with(nolock) where movimientos_internos.folio=@recepcion and movimientos_internos.transaccion='65')and movimientos_internos.transaccion='35')


select * from (
	
select  recepcionados.cod_prod
       ,productos.descripcion as producto
       ,case when(select sum(entysal.cantidad) from entysal with(nolock) where entysal.folio=movimientos_internos.folio_alterno and entysal.cod_prod=recepcionados.cod_prod  and entysal.transaccion='35')is null then 0 
             else (select sum(entysal.cantidad) from entysal with(nolock) where entysal.folio=movimientos_internos.folio_alterno and entysal.cod_prod=recepcionados.cod_prod and entysal.transaccion='35')end as transferidos
       ,sum(recepcionados.cantidad) as recepcionados 
       ,sum(recepcionados.cantidad)-(case when(select sum(entysal.cantidad) from entysal with(nolock) where entysal.folio=movimientos_internos.folio_alterno and entysal.cod_prod=recepcionados.cod_prod and entysal.transaccion='35' )is null then 0 
             else (select sum(entysal.cantidad) from entysal with(nolock) where entysal.folio=movimientos_internos.folio_alterno and entysal.cod_prod=recepcionados.cod_prod and entysal.transaccion='35')end )as diferiencia
      ,estab_alterno.nombre as establecimiento_transfiere 
      ,establecimientos.nombre as establecimiento_recepciona 
      , case when(sum(recepcionados.cantidad)-(case when(select sum(entysal.cantidad) from entysal with(nolock) where entysal.folio=movimientos_internos.folio_alterno and entysal.cod_prod=recepcionados.cod_prod and entysal.transaccion='35')is null then 0 
                    else (select sum(entysal.cantidad) from entysal with(nolock) where entysal.folio=movimientos_internos.folio_alterno and entysal.cod_prod=recepcionados.cod_prod and entysal.transaccion='35')end ))<0 
               then 'FALTANTES' 
             when(sum(recepcionados.cantidad)-(case when(select sum(entysal.cantidad) from entysal with(nolock) where entysal.folio=movimientos_internos.folio_alterno and entysal.cod_prod=recepcionados.cod_prod and entysal.transaccion='35')is null then 0 
                   else (select sum(entysal.cantidad) from entysal with(nolock) where entysal.folio=movimientos_internos.folio_alterno and entysal.cod_prod=recepcionados.cod_prod and entysal.transaccion='35')end ))=0 
              then 'ZRECEPCIONADO COMPLETO' 
            else 'SOBRANTES' end  as movimiento 
     ,@recepcion as recepcion 
	 ,movimientos_internos.folio_alterno as folio_transferencia
	 ,@fecha_transferencia as fecha_tranferencia
	 ,@fecha_recepcion as fecha_recepcion

 from movimientos_internos with(nolock)
	inner join entysal as recepcionados with(nolock) on recepcionados.folio=movimientos_internos.folio and recepcionados.transaccion='65'
	inner join productos on productos.cod_prod=recepcionados.cod_prod
	inner join establecimientos on establecimientos.cod_estab=movimientos_internos.cod_estab
	inner join establecimientos as estab_alterno on estab_alterno.cod_estab=movimientos_internos.cod_estab_alterno
 where movimientos_internos.folio=@recepcion  
 group by recepcionados.cod_prod,productos.descripcion,movimientos_internos.folio_alterno,estab_alterno.nombre ,establecimientos.nombre,movimientos_internos.folio_alterno

 UNION ALL

select  transferidos.cod_prod
       ,productos.descripcion as producto
       ,transferidos.cantidad as    transferidos
       ,0 as recepcionados 
       ,transferidos.cantidad *-1    as diferiencia
       ,estab_alterno.nombre as establecimiento_transfiere 
       ,establecimientos.nombre as establecimiento_recepciona 
       ,'FALTANTE COMPLETO' as movimiento
       ,@recepcion as recepcion
	   ,movimientos_internos.folio_alterno as folio_transferencia
	   ,@fecha_transferencia as fecha_tranferencia
	   ,@fecha_recepcion as fecha_recepcion

            
from movimientos_internos with(nolock)
 
inner join entysal as transferidos with(nolock) on transferidos.folio=movimientos_internos.folio_alterno  and transferidos.transaccion='35'
inner join productos on productos.cod_prod=transferidos.cod_prod
inner join establecimientos on establecimientos.cod_estab=movimientos_internos.cod_estab
inner join establecimientos as estab_alterno on estab_alterno.cod_estab=movimientos_internos.cod_estab_alterno

where movimientos_internos.folio=@recepcion and transferidos.cod_prod not in (select  recepcionados.cod_prod from movimientos_internos with(nolock) inner join entysal as recepcionados with(nolock) on recepcionados.folio=movimientos_internos.folio
                                                                                where movimientos_internos.folio=@recepcion) 
                                                                                
) a
where a.recepcionados >0

order by  a.movimiento asc]]>
	</queryString>
