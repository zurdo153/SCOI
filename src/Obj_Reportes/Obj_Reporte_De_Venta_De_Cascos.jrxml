	<queryString language="SQL">
		<![CDATA[declare @folio int = 8

SELECT tb_venta_de_cascos_proveedores.[folio_venta_casco]
      ,tb_venta_de_cascos_proveedores.[cod_prv]
      ,tb_venta_de_cascos_proveedores.[proveedor]
      ,tb_venta_de_cascos_proveedores.[beneficiario_proveedor]
      ,tb_entysal.[transaccion_origen]
      ,convert(varchar(20),tb_entysal.fecha,103)+' '+convert(varchar(20),tb_entysal.fecha,108) as fecha_movimiento
      ,tb_entysal.folio_producto
	  ,tb_productos.descripcion
      ,tb_entysal.cantidad
      ,tb_entysal.costo
      ,tb_entysal.precio_venta
      ,(tb_entysal.precio_venta*tb_entysal.cantidad)*-1 as importe
      ,tb_entysal.status
	  ,tb_venta_de_cascos_proveedores.total
	  ,(((tb_entysal.precio_venta*tb_entysal.cantidad)*-1)-((tb_entysal.costo*tb_entysal.cantidad)*-1)) as bonificacion 
      ,(select nombre+' '+ap_paterno+' '+ap_materno from tb_empleado where folio=[usuario_realizo_mov])as nombre_usuario_realizo_movimiento
  FROM  tb_venta_de_cascos_proveedores
  inner join [tb_entysal] on [tb_entysal].folio_origen=tb_venta_de_cascos_proveedores.[folio_venta_casco] and tb_entysal.transaccion_origen=24
  inner join tb_productos on tb_productos.folio_producto=[tb_entysal].folio_producto

   where tb_venta_de_cascos_proveedores.folio_venta_casco=@folio
]]>
	</queryString>
