	<queryString language="SQL">
		<![CDATA[select pedido.folio_pedido
,estab_ori.establecimiento as origen
,estab_des.establecimiento as destino
,productos.codigo_barras_pieza as codigo_alternativo
,pedido.cod_prod
,pedido.descripcion
,pedido.surtido
,pedido.unidad
,surtio.nombre+' '+surtio.ap_paterno+' '+surtio.ap_materno as surtio
,asigno.nombre+' '+asigno.ap_paterno+' '+asigno.ap_materno as asigno
,convert(varchar(20),pedido.fecha_de_asignacion,103)+' '+convert(varchar(20),pedido.fecha_de_asignacion,108) fecha_de_asignacion
,pagina.folio_empleado_asignado
,emp_asignado_a_pagina.nombre+' '+emp_asignado_a_pagina.ap_paterno+' '+emp_asignado_a_pagina.ap_materno as asignado_a_pagina 
,isnull(clasif.descripcion,'Sin clasificador') as clasificador
,CASE WHEN pedido.status='A' THEN 'ASIGNADO'
		WHEN pedido.status='S' THEN 'SURTIDO'
		WHEN pedido.status='C' THEN 'CANCELADO'
		WHEN pedido.status='N' THEN 'NEGADO'
	ELSE 'VIGENTE' END AS status
from tb_gestion_de_pedido pedido
inner join IZAGAR_establecimientos_calculo estab_ori on estab_ori.cod_estab = pedido.establecimiento_origen
inner join IZAGAR_establecimientos_calculo estab_des on estab_des.cod_estab = pedido.establecimiento_destino
inner join tb_empleado surtio on surtio.folio = pedido.folio_usuario_capturo
inner join tb_empleado asigno on asigno.folio = pedido.folio_usuario_asigno
inner join tb_asignacion_de_pedido_por_paginas pagina on pagina.folio_pedido = pedido.folio_pedido and pagina.numero_de_pagina = pedido.numero_de_pagina
inner join tb_empleado emp_asignado_a_pagina on emp_asignado_a_pagina.folio = pagina.folio_empleado_asignado
inner join [192.168.2.201].BMSIZAGAR.dbo.productos productos with(nolock) on productos.cod_prod = pedido.cod_prod
left outer join tb_clasificacion_de_pedidos_de_establecimientos clasif on clasif.folio = pedido.folio_clasificador
where pedido.folio_pedido = 'S208040'
and surtido  > 0]]>
	</queryString>
