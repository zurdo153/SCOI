	<queryString>
		<![CDATA[select  tb_empleado.folio as folio_empleado
       ,tb_empleado.nombre+' '+tb_empleado.ap_paterno+' '+tb_empleado.ap_materno as empleado
       ,tb_establecimiento.nombre as establecimiento
       ,tb_sueldo.sueldo
       ,tb_bono.bono
       ,infonavit
       ,(select max(numero_lista)+1 from tb_lista_raya)  as lista_de_raya
       ,(select convert(varchar(15),fecha_lista_raya_pasada+1,103) from tb_configuracion_sistema) as fecha_inicial_lista_raya
       ,(select convert(varchar(15),fecha_lista_raya_pasada+7,103) from tb_configuracion_sistema) as fecha_final_lista_raya
       ,'Empleados Con Infonavit De Lista De Raya Actual' as reporte

from tb_empleado
inner join tb_sueldo on tb_sueldo.folio=tb_empleado.sueldo_id
left outer join tb_bono on tb_bono.folio=tb_empleado.bono_id
left outer join tb_establecimiento on tb_establecimiento.folio=tb_empleado.establecimiento_id
where tb_empleado.status=1 and infonavit>0]]>
	</queryString>
