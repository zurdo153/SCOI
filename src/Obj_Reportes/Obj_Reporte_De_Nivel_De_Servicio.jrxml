	<queryString language="SQL">
		<![CDATA[--exec sp_Reporte_De_Nivel_De_Servicio '01/10/2016 00:00:00','15/10/2016 23:59:59'
declare @fecha_in datetime,@fecha_fin datetime

set @fecha_in = '01/10/2016 00:00:00'
set @fecha_fin= '15/10/2016 23:59:59'

select convert(varchar(20),@fecha_in,103) as fecha_inicial
		,convert(varchar(20),@fecha_fin,103) as fecha_final
		,estab_ori.establecimiento pidio
		,estab_des.establecimiento surte
		,count(b.folio_pedido) as pedidos
		,sum(b.cantidad_de_filas) as cantidad_de_filas
		,sum(b.negados) as negados
		--,convert(numeric(15,4),(convert(float,sum(b.negados))/sum(b.cantidad_de_filas))) as porcetage_de_negados
		,sum(b.surtidos) as surtidos
		--,convert(numeric(15,4),(convert(float,sum(b.cantidad_de_filas)-sum(b.negados))/sum(b.cantidad_de_filas))) as porcetage_de_surtidos

		,isnull((select count(folio_pedido) 
			from tb_gestion_de_pedido pedido 
			where convert(varchar(20),pedido.fecha_de_captura,103) between @fecha_in and @fecha_fin 
			and pedido.ajuste < 0
			and pedido.establecimiento_origen = b.pidio
			and pedido.establecimiento_destino = b.surte
			group by pedido.establecimiento_origen,pedido.establecimiento_destino),0) ajuste

		,convert(numeric(15,4),(convert(float,(isnull((select count(folio_pedido) 
			from tb_gestion_de_pedido pedido 
			where convert(varchar(20),pedido.fecha_de_captura,103) between @fecha_in and @fecha_fin 
			and pedido.ajuste < 0
			and pedido.establecimiento_origen = b.pidio
			and pedido.establecimiento_destino = b.surte
			group by pedido.establecimiento_origen,pedido.establecimiento_destino),0) ))/sum(b.surtidos))) as porcetage_de_ajuste
from (select a.folio_pedido
						,a.pidio
						,a.surte
						,a.cantidad_de_filas
						,(select count(surtido) from tb_gestion_de_pedido where folio_pedido = a.folio_pedido and surtido = 0) as negados
						,(select count(surtido) from tb_gestion_de_pedido where folio_pedido = a.folio_pedido and surtido > 0) as surtidos
				 from (select pedido.folio_pedido
										,pedido.establecimiento_origen as pidio
										,pedido.establecimiento_destino as surte
										,count(pedido.folio_pedido) AS cantidad_de_filas
								from tb_gestion_de_pedido pedido
								where convert(varchar(20),pedido.fecha_de_captura,103) between @fecha_in and @fecha_fin
								group by pedido.folio_pedido,pedido.establecimiento_origen,pedido.establecimiento_destino) a
				group by a.folio_pedido,a.pidio,a.surte,a.cantidad_de_filas) b
				inner join IZAGAR_establecimientos_calculo estab_ori on estab_ori.cod_estab = pidio
				inner join IZAGAR_establecimientos_calculo estab_des on estab_des.cod_estab = surte
group by b.pidio,b.surte,estab_ori.establecimiento ,estab_des.establecimiento]]>
	</queryString>
