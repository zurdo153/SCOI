		<queryString>
			<![CDATA[declare @anios int, @meses int, @dias int, @folio_empleado int

set @folio_empleado =(select dos from tb_ids);

set @dias =(select (day(getdate())-day(tb_empleado.fecha_nacimiento)) from tb_empleado where folio = @folio_empleado)
set @meses =(select (month(getdate())-month(tb_empleado.fecha_nacimiento)) from tb_empleado where folio = @folio_empleado)
set @anios =(select (year(getdate())-year(tb_empleado.fecha_nacimiento)) from tb_empleado where folio = @folio_empleado)

select tb_empleado.folio as folio
,tb_empleado.nombre as nom_empleado
,tb_empleado.ap_paterno+' '+tb_empleado.ap_materno as ap_empleado
,tb_empleado.no_checador as codigo
,convert(varchar(20),tb_empleado.fecha_ingreso,103) as fecha_ingreso
,tb_empleado.imss as imss
,tb_empleado.foto as foto
,tb_puesto.nombre as puesto
,(select case when (@meses)<0 
				then (@anios-1)
			when (@meses)=0 
					then (select case when (@dias)<0 
										then (@anios-1)
									  else @anios
								 end)
			else @anios
		end) as edad
,tb_empleado.curp as curp
from tb_empleado
inner join tb_puesto on tb_puesto.folio = tb_empleado.puesto_id
where tb_empleado.folio =@folio_empleado]]>
